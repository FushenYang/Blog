{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "title": "books",
      "path": "[[workspace]]/src/content/books"
    },
    {
      "title": "devs",
      "path": "[[workspace]]/src/content/devs"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": "'blog'",
      "filePrefix": null,
      "clearEmpty": true,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "pubDate",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags",
          "default": ""
        }
        ,
        {
          "title": "path",
          "name": "path",
          "type": "string",
          "default": ""
        }
        ,
        {
          "title": "author",
          "name": "author",
          "type": "string",
          "default": "Fushen YANG"
        }
      ]
    }
  ]
}